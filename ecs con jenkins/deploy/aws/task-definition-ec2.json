{
    "executionRoleArn": "arn:aws:iam::397748689898:role/ecsTaskExecutionRole",
    "memory": "512",

    "cpu": "256",
    
    "taskRoleArn": "arn:aws:iam::397748689898:role/ecsTaskExecutionRole",
    "family":"%APPNAME%", 
    "containerDefinitions": [
        { 
            "name": "%APPNAME%",
            "image": "%ECRIMAGEN%",
            "cpu": 256,
            "memoryReservation": 512,
          
            "logConfiguration": {
                "logDriver": "awslogs",
            
                "options": {
                    "awslogs-group": "%APPNAME%",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
           
            "portMappings": [
                {
                    "hostPort": %IMAGEPORT%,
                    "protocol": "tcp",
                    "containerPort": %IMAGEPORT%
                }
            ],         
            "environment": [],          
            "essential": true,          
            "readonlyRootFilesystem": false
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "host",
    "volumes": []
  }